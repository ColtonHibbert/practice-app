<%= form_with scope: :poke_article, url: poke_articles_path, local: true do |form| %>
    <p>
        <%= form.label :title %><br>
        <%= form.text_field :title %>
    </p>
    <p>
        <%= form.label :text %><br>
        <%= form.text_area :text %><br>
    </p>
    <%= form.submit %>
<% end %>